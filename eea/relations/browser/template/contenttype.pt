<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US"
  lang="en-US"
  metal:use-macro="here/main_template/macros/master"
  i18n:domain="eea">

<body>

<div metal:fill-slot="content-core">

    <dl class="portalMessage error" tal:define="bad_relations here/@@graph.png/markBrokenRelations" tal:condition="bad_relations" id="bad_relations">
        <dt i18n:translate="">Error</dt>
        <dd><tal:relations tal:content="bad_relations" /></dd>
    </dl>

<div style="width: 100%; overflow: auto;">
  <div style="text-align: center; border: 1px solid #8CACBB; padding: 2em 0">
  <img tal:attributes="
    src string:${here/absolute_url}/@@graph.png;
    alt string:Relation Graph;
    title string:Relation Graph" />
  </div>
</div>

</div>
</body>
</html>
